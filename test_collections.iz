// Test Array enhancements
print("=== Array Methods ===");

var arr = [1, 2, 3, 4, 5];
print("Original array:", arr);

// Test shift()
var first = shift(arr);
print("After shift():", arr, "- removed:", first);

// Test unshift()
unshift(arr, 0);
print("After unshift(0):", arr);

// Test splice() - remove 2 elements starting at index 2
var removed = splice(arr, 2, 2);
print("After splice(2, 2):", arr, "- removed:", removed);

// Test splice() - remove from index 1 to end
var arr2 = [10, 20, 30, 40, 50];
var removed2 = splice(arr2, 1);
print("After splice(1) on [10,20,30,40,50]:", arr2, "- removed:", removed2);

print("");
print("=== Map Methods ===");

var person = {"name": "Alice", "age": 30, "city": "NYC"};
print("Original map:", person);

// Test has() - alias for hasKey()
print("has(person, 'name'):", has(person, "name"));
print("has(person, 'country'):", has(person, "country"));

// Test delete()
var deleted = delete(person, "city");
print("After delete('city'):", person, "- existed:", deleted);

// Test entries()
var ents = entries(person);
print("entries():", ents);

print("");
print("=== Set Type ===");

var mySet = Set();
print("Empty set:", mySet);

// Test setAdd()
setAdd(mySet, "apple");
setAdd(mySet, "banana");
setAdd(mySet, "cherry");
setAdd(mySet, "apple"); // duplicate, should not add twice
print("After adding items:", mySet);

// Test setSize()
print("Set size:", setSize(mySet));

// Test setHas()
print("setHas(mySet, 'banana'):", setHas(mySet, "banana"));
print("setHas(mySet, 'grape'):", setHas(mySet, "grape"));

// Test setDelete()
var wasDeleted = setDelete(mySet, "banana");
print("After deleting 'banana':", mySet, "- existed:", wasDeleted);
print("Final set size:", setSize(mySet));

// Test with numbers
var numSet = Set();
setAdd(numSet, 1);
setAdd(numSet, 2);
setAdd(numSet, 3);
setAdd(numSet, 1); // duplicate
print("Number set:", numSet);
print("Number set size:", setSize(numSet));
