// Test both break and continue
print("Testing break and continue together:");
var sum = 0;
var i = 0;

while (i < 20) {
    i = i + 1;
    
    // Skip 3, 5, 7, 9
    if (i == 3) {
        continue;
    }
    if (i == 5) {
        continue;
    }
    if (i == 7) {
        continue;
    }
    if (i == 9) {
        continue;
    }
    
    // Break at 15
    if (i == 15) {
        print("Breaking at", i);
        break;
    }
    
    sum = sum + i;
    print("Adding", i, "sum =", sum);
}

print("Final sum:", sum);
print("Final i:", i);
