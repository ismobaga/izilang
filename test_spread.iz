// Test spread operator in arrays
var arr1 = [1, 2, 3];
var arr2 = [4, 5, 6];
var combined = [...arr1, ...arr2];
print(combined);

// Test spread with mixed elements
var mixed = [0, ...arr1, 7, ...arr2, 8];
print(mixed);

// Test spread operator in maps
var obj1 = {"name": "John", "age": 30};
var obj2 = {"city": "NYC", "country": "USA"};
var merged = {...obj1, ...obj2};
print(merged);

// Test spread with override
var override = {...obj1, "age": 31, ...obj2};
print(override);
