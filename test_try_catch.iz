// Test basic try-catch
print("Test 1: Basic try-catch");
try {
    print("In try block");
    throw "Test exception";
    print("After throw (should not print)");
} catch(e) {
    print("Caught exception: ");
    print(e);
}
print("After try-catch");
print("");

// Test try-catch with no exception
print("Test 2: Try-catch with no exception");
try {
    print("No exception thrown");
} catch(e) {
    print("Should not catch");
}
print("Completed successfully");
print("");

// Test try-finally
print("Test 3: Try-finally");
try {
    print("In try block with finally");
} finally {
    print("Finally block executed");
}
print("After try-finally");
print("");

// Test try-catch-finally
print("Test 4: Try-catch-finally");
try {
    print("In try block");
    throw "Error in try";
} catch(e) {
    print("Caught: ");
    print(e);
} finally {
    print("Finally always executes");
}
print("After try-catch-finally");
print("");

// Test nested try-catch
print("Test 5: Nested try-catch");
try {
    print("Outer try");
    try {
        print("Inner try");
        throw "Inner exception";
    } catch(e) {
        print("Inner catch: ");
        print(e);
    }
    print("Between inner and outer");
} catch(e) {
    print("Outer catch (should not execute)");
}
print("Done with nested test");
