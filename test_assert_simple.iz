// Simple test for std.assert module without exception handling

import * as assert from "std.assert";

print("Testing assert.ok with truthy values...");
assert.ok(true);
assert.ok(1);
assert.ok("hello");
print("✓ Passed");

print("");
print("Testing assert.eq with equal values...");
assert.eq(1, 1);
assert.eq("hello", "hello");
assert.eq(true, true);
print("✓ Passed");

print("");
print("Testing assert.ne with different values...");
assert.ne(1, 2);
assert.ne("hello", "world");
assert.ne(true, false);
print("✓ Passed");

print("");
print("All basic tests passed!");
