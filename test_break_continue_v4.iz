// Test both break and continue
print("Testing:");
var i = 0;
while (true) {
    i = i + 1;
    print("i =", i);
    
    if (i == 3) {
        print("Skipping 3");
        continue;
    }
    
    if (i == 5) {
        print("Breaking at 5");
        break;
    }
    
    print("Processed", i);
}

print("Final i:", i);
