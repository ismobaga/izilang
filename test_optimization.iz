// Test constant folding optimization
var a = 2 + 3;  // Should be folded to 5
print(a);

var b = 10 - 4;  // Should be folded to 6
print(b);

var c = 5 * 6;  // Should be folded to 30
print(c);

var d = 20 / 4;  // Should be folded to 5
print(d);

var e = -(2 + 3);  // Should be folded to -5
print(e);

var f = !(1 > 2);  // Should be folded to true
print(f);

// Test dead code elimination
if (true) {
    print("This should execute");
} else {
    print("This should be eliminated");
}

if (false) {
    print("This should be eliminated");
} else {
    print("This should execute");
}

// Test dead code after return
fn testDeadCode() {
    print("Before return");
    return 42;
    print("This should be eliminated");
}

testDeadCode();

print("All optimization tests passed!");
