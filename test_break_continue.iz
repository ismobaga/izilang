// Test both break and continue
print("Testing break and continue together:");
var sum = 0;
var i = 0;

while (i < 20) {
    i = i + 1;
    
    // Skip odd numbers
    if (i == 3 or i == 5 or i == 7 or i == 9) {
        continue;
    }
    
    // Break at 15
    if (i > 14) {
        print("Breaking at", i);
        break;
    }
    
    sum = sum + i;
    print("Adding", i, "sum =", sum);
}

print("Final sum:", sum);
print("Final i:", i);
